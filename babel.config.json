{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@exemple": "./src/exemple",
          "@modules": "./src/modules"
        }
      }
    ]
  ],
  "ignore": [
    "**/*.spec.ts"
  ]
}